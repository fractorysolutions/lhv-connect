{
    "name": "fractorysolutions/lhv-connect",
    "description": "LHV bank's CONNECT service for Laravel 6+, fork of mihkullorg/lhv-connect",
    "keywords": ["lhv", "lhv-connect", "connect", "lhv-bank", "bank"],
    "homepage": "https://github.com/fractorysolutions/lhv-connect",
    "license": "MIT",
    "authors": [{
        "name": "Mihkel Allorg",
        "email": "mihkel.allorg@gmail.com",
        "role": "Developer"
    },
    {
        "name": "Rao Zvorovski",
        "email": "rao@fractory.com",
        "role": "Developer"
    }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/config": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mihkullorg\\LhvConnect\\": "src/Mihkullorg/LhvConnect"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mihkullorg\\LhvConnect\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mihkullorg\\LhvConnect\\LaravelLhvConnectServiceProvider"
            ]
        }
    }
}
